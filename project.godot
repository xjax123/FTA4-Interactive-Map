config_version=5

[application]

config/name="FTA4-Map"
run/main_scene="res://Scenes/system_viewer.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

PlanetEnums="*res://Globals/PlanetEnums.gd"
SysView="*res://Globals/SysView.gd"
AntialiasedLine2DTexture="*res://addons/antialiased_line2d/texture.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/borderless=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="FTA4-Map"

